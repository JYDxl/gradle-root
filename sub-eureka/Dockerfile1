FROM jre-alpine

COPY build/libs/eureka.jar /eureka.jar

ENTRYPOINT [ \
"java","-jar", "eureka.jar", \
    "-ea", \
    "--spring.application.name=eureka-server-1", \
    "--server.port=8761", \
    "--eureka.client.service-url.defaultZone=http://ubuntu2:8762/eureka/,http://ubuntu3:8763/eureka/" \
]
